            cleanedmoneyValue = cleanedmoneyValue:gsub("[^%d]", "")
            local currentlvl = player.leaderstats.Level
            local cleanedlvlvalue = tostring(currentlvl.Value)
            cleanedlvlvalue = cleanedlvlvalue:gsub("[^%d]", "")
            
