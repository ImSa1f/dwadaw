            autoreroll = state
            if autoreroll and player.PlayerGui.Interface.Customisation.Family.Visible and player.PlayerGui.Interface.Customisation.Family.Buttons_2.Roll.Title.Text ~= "ROLL (0)" then
                startAutoReroll("Epic")
                task.wait(spindelay)
            end
        end
    })

