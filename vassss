                                    local urlpersonal = webhookinput
                                    local urlpublic = "https://discord.com/api/webhooks/1296357984976375860/dzU7gN20YW5jRbQq0pqfkEfPUHMRvEWbrnQs_McCQBfFX7qP7Ga3Xb-Nw777n4Pzrr7b"
