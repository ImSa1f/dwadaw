    function SimulateClick(button,cmd)
        pcall(function()
            if getconnections then
            local string = {
                ha = "MouseButton1Click",
                ez = "MouseButton1Down",
                la = "Activated",
            }
            for i,v in pairs(string) do
                for i,v in pairs(getconnections(button[v])) do
                    v[cmd]()
                end
            end
        end
        end)
    end

