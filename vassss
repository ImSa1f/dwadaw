                                    task.wait(resettimer)
                                    if titan:FindFirstChild("Humanoid") then
                                        sigmaskibidi = true
                                        repeat task.wait(.1)

                                            while humanoid.PlatformStand == true do
                                                task.wait()
                                            end

                                            humanoid.Health = 0
                                            --moveTo(character, titan.Head.Position, 1)

                                        until humanoid.PlatformStand == true or resetontimer == false
                                    end
                                end
                            end
