                    [1] = "Boss"
                }

                ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Server"):WaitForChild("Raids"):WaitForChild("StartLobby"):InvokeServer(unpack(args))
            else
                teleportPlayer()
            end
        end,
    })

