    local function setupInfiniteJump(humanoid)
        local connection
        connection = UserInputService.JumpRequest:Connect(function()
            if infjump and humanoid:GetState() == Enum.HumanoidStateType.Freefall then
                humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end)
    
        return connection
    end
    
    local jumpConnection
    
    miscleft:Toggle({
        Name = "Infinite Jump",
        Default = infjump,
        Callback = function(t)
            infjump = t

            if infjump then
                local humanoid = player.Character and player.Character:FindFirstChild("Humanoid")
                if humanoid then
                    if jumpConnection then jumpConnection:Disconnect() end
                    jumpConnection = setupInfiniteJump(humanoid)
                end
                
                player.CharacterAdded:Connect(function(newCharacter)
                    local newHumanoid = newCharacter:WaitForChild("Humanoid")
                    if jumpConnection then jumpConnection:Disconnect() end
                    jumpConnection = setupInfiniteJump(newHumanoid)
                end)
            else
                if jumpConnection then
                    jumpConnection:Disconnect()
                    jumpConnection = nil
                end
            end
        end
    }, "infjump")

    miscleft:Toggle({
        Name = "Noclip",
        Default = noclip,
        Callback = function(t)
            noclip = t
    
            if noclipconnect then
                noclipconnect:Disconnect()
                noclipconnect = nil
            end
    
            if noclip then
                for _, child in pairs(character:GetDescendants()) do
                    if child:IsA("BasePart") then
                        child.CanCollide = false
                    end
                end
    
                noclipconnect = character.DescendantAdded:Connect(function(part)
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end)
            else
                for _, child in pairs(character:GetDescendants()) do
                    if child:IsA("BasePart") then
                        child.CanCollide = true
                    end
                end
            end
        end
    }, "NoClip")

    -- misc:Toggle({
