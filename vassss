                    [3] = DiffSelected,
                    [4] = ""
                }

                ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Server"):WaitForChild("Raids"):WaitForChild("CreateLobby"):InvokeServer(unpack(args))
