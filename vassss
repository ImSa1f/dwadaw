                if v:IsA("Part") and v.Name == AREATOTPTO then
                    local areaPos = v.Position
                    if areaPos then
                        humanoidRootPart.CFrame = CFrame.new(areaPos) * CFrame.new(0,5,0)
                    end
                end
            end
        end
